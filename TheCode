                    print('You now have', money, '!')
            else:
                print('Wrong answer! You lose!')
                print("You have won ", money)
                file = open("BillionaireScores.txt", "a")
                scoreText = (anyName + " won" + str(money) + "\n")
                file.write(scoreText)
                file.close()
                exit()

        elif choice == 100:
