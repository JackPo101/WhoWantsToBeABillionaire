def menu():
    anyName = input('What is your name?')
    print('Hi', anyName, '! Welcome to Who Wants To Be A Billionaire')
    time.sleep(1)
    print('                                                             ')
    print('You will be presented by random questions from certain categories.')
    print('                                                           ')
    time.sleep(1)
    print('The categories are:')
    print('                         ')
    time.sleep(1)
    print('General Knowledge')
    print('                 ')
    time.sleep(1)
    print('Science')
    print('                 ')
    time.sleep(1)
